<?xml version="1.0" encoding="UTF-8"?>
<Context path="/dawFileUpload">
  <Resources>
    <!-- Tomcat only: Alias URL for images
             Change base atribute with and absolute path in filesystem
             webAppMount is relative to URL Path, i.e. dawFileUpload/images
        -->
    <PostResources base="/tmp/images" className="org.apache.catalina.webresources.DirResourceSet" webAppMount="/images"/>
  </Resources>
</Context>
